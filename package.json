{
    "name": "cougarecho",
    "version": "1.0.0",
    "main": "server/server.js",
    "type": "module",
    "scripts": {
        "gcp-build": "npm install --prefix ./client && npm run build --prefix ./client && npm install --prefix ./server",
        "start": "npm run start --prefix ./server",
        "client": "cd client && npm run dev",
        "server": "cd server && npm start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "jsonwebtoken": "^9.0.2",
        "mssql": "^11.0.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}